{
  "name": "testing",
  "version": "1.0.0",
  "description": "A simple Express server for portfolio hosting",
  "main": "dist/index.js",
  "scripts": {
    "launch": "node dist/index.js",
    "start:dev": "ts-node src/index.ts",
    "build": "tsc",
    "dev": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
    "render-start": "npm run build && npm run launch"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "dotenv": "^17.0.1",
    "express": "^5.1.0",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.0"
  }
}
